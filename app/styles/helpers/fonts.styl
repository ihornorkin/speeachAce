@font-face
	font-family 'icomoon'
	src url('../fonts/icon-font/icomoon.eot?9eq6ro')
	src url('../fonts/icon-font/icomoon.eot?9eq6ro#iefix') format('embedded-opentype'),
			url('../fonts/icon-font/icomoon.ttf?9eq6ro') format('truetype'),
			url('../fonts/icon-font/icomoon.woff?9eq6ro') format('woff'),
			url('../fonts/icon-font/icomoon.svg?9eq6ro#icomoon') format('svg')
	font-weight normal
	font-style normal

[class^="icon-"], [class*=" icon-"]
	font-family 'icomoon' !important
	speak none
	font-style normal
	font-weight normal
	font-variant normal
	text-transform none
	line-height 1
