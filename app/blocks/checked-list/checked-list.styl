.checked-list
	display block
	list-style-type none
	margin 0
	padding 0

	li
		font-size 1.6rem
		font-weight 500
		line-height 1.5
		margin-bottom 2.3rem
		padding-left 3.3rem
		position relative

		&::before
			color azure
			content '\ea10'
			font-family icon-font
			left 0
			position absolute
			top 0
